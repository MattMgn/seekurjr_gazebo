<!-- JOINTS -->

<joint name="bl_wheel_joint" type="continuous">
<origin xyz="-0.215 0.32 0.083" rpy="0 0 0"/>
<parent link="base_link"/>
<child link="bl_wheel"/>
<axis xyz="0 1 0"/>
<dynamics damping="1.0" friction="0.0" />
<limit effort="30" velocity="1.0"  lower="-100" upper="100" />
</joint>

