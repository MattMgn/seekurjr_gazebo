<!-- BASE FOOTPRINT -->
<!-- 
base_footprint is a fictitious link(frame) that is on the 
ground right below base_link origin,navigation stack depends on this frame 
-->

<!-- BASE_LINK (RED) -->
<link name="base_link">

	<inertial>
		<mass value="70.0" />
		<origin xyz="0 0 0" rpy="1.570796 0 1.570796"/>
		<inertia ixx="0.011083" ixy="-0.000132"  ixz="-0.000303"  iyy="0.011182"  iyz="-0.000597"  izz="0.005307" />
	</inertial>

	<visual>
		<origin xyz="0 0 0" rpy="1.570796 0 1.570796" />
		<geometry>
			<mesh filename="package://seekurjr_gazebo/meshes/seekurjr_body_new_low2.dae" scale="1 1 1"/>
		</geometry>
	</visual>

	<collision>
		<origin xyz="0 0 0" rpy="1.570796 0 1.570796" />
		<geometry>
			<mesh filename="package://seekurjr_gazebo/meshes/seekurjr_body_new_low2.stl" scale="1 1 1"/>
		</geometry>
	</collision>     

</link>
  
