<?xml version="1.0"?>

<launch>
<!-- Convert xacro and put on parameter server -->
<param name="robot_description" command="$(find xacro)/xacro.py $(find seekurjr_gazebo)/urdf/seekurjr.xacro.xml" />

<!-- Spawn a robot into Gazebo -->
<node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model seekurjr" />
</launch>


